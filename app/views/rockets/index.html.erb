<h1>All destinations</h1>

<% @rockets.each do |rocket| %>
<ul>
  <li> Destination: <%= link_to rocket.destination, rocket_path(rocket) %></li>
  <li><%= link_to rocket.rocket_type, rocket_path(rocket) %></li>
  <%= link_to "Book Now", new_rocket_booking_path(rocket) %>
  <li><%= link_to "Delete", rocket_path(rocket),
  method: :delete,
  data: { confirm: "Are you sure?"} %></li>
</ul>
<% end %>
<p><%= link_to "Create a new one", new_rocket_path %></p>

