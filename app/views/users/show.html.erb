    <li><p>This are your owner details:</p></li>
    <li>Company: <%= current_user.owner.company %> | <%= link_to "Edit", edit_user_owner_path(current_user) %> </li>

  <% else %>
    <%= link_to "Create your owner profile", new_user_owner_path(current_user) %>
  <% end %>
</ul>

